/* pages/management/calendar_type_management.wxss */

.head_calendar_type_management {
  height: 90rpx;
  width: 100%;
  font-size: 11pt;
  display: flex;
  justify-content: center;
  text-align: center;
  align-items: center;
  padding: 20rpx 0 ;
  position: fixed;
  left: 0;
  background-color: #eee;
}

.list_calendar_type_management_view {
  width: 100%;
  padding-top: 130rpx;
}

.box_head {
  width: 90%;
  height: 100%;
  align-items: center;
  color: #666;
  background: #fdfdfd;
  display: flex;
  border-radius: 10rpx;
  flex-direction: row;
  justify-content: space-between;
  padding: 0 30rpx;
}

/*
日历类型管理页面 列表样式
  */

.list_calendar_type_management {
  width: 100%;
  margin: 10rpx 0 0 0 ;
  display: flex;
  justify-content: center;
  text-align: center;
  align-items: center;
  font-size: 11pt;
}

.choosed_type_line {
  color: white;
  background-color: #09bb07 !important;
}

.title_box_list {
  width: 100%;
  height: 90rpx;
  background-color: #fdfdfd;
  flex-direction: row;
  padding: 0px 15px 0px 15px;
  display: flex;
  align-items: center;
  justify-content:space-between;
}

.type_detail {
  display: flex;
  line-height: 30px;
  align-items: center;
}

.type_num {
  color: white;
  background-color: orange;
  border-radius: 30px;
  height: 30px;
  width: 30px;
  justify-content: flex-end;
  margin-left: 15px;
}

/* pages/management/calendar_type_management.wxss */

.head_calendar_type_management {
  height: 90rpx;
  width: 100%;
  font-size: 11pt;
  display: flex;
  justify-content: center;
  text-align: center;
  align-items: center;
  margin: 20rpx 0 ;
}

.box_head {
  width: 90%;
  height: 100%;
  align-items: center;
  color: #666;
  background: #fdfdfd;
  display: flex;
  border-radius: 10rpx;
  flex-direction: row;
  justify-content: space-between;
  padding: 0 30rpx;
}

/*
日历类型管理页面 列表样式
  */

.list_calendar_type_management {
  width: 100%;
   margin: 10rpx 0 0 0 ;
  display: flex;
  justify-content: center;
  text-align: center;
  align-items: center;
  font-size: 11pt;
}

.title_box_list {
  width: 100%;
  height: 90rpx;
  background-color: #fdfdfd;
  flex-direction: row;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
}
